function five_point_gauss_seidel(a_x,b_x,a_y,b_y,h,k,bc,F)
y = [a_y:k:b_y];
x = [a_x:h:b_x];
m = length(x);
n = length(y);
U = zeros(m,n):
%Boundary Condition
j=1;
for i=1:m
    U(i,j) = bc(x(i),y(j));
end
j = n;
for i=1:m
    U(i,j) = bc(x(i),y(j));
end

i = 1;
for j=1:n
    U(i,j) = bc(x(i),y(j));
end
i = m;
for j=1:n
    U(i,j) = bc(x(i),y(j));
end
%%End of Boundary Condition
%Gauss - Seidel Iterations
tol = 1e-3;
curr_max_error = 100;
while curr_max_error > tol
for i=1:m
    for j=1:n
        U(i,j) = 0.25*(U(i-1,j) + U(i+1,j) + U(i,j-1) + U(i,j+1)) - 

end